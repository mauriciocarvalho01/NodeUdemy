{
  "name": "apl-back-serv-ces",
  "version": "1.0.13",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "startOld": "npm run serve",
    "start": "node -r ts-node/register src/index.ts",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/index.js",
    "watch-node": "node ./node_modules/nodemon/bin/nodemon.js dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "testOld": "node ./node_modules/mocha/bin/mocha ./dist/test/unit/*Test.js",
    "test": "npm run test-ts",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "integration": "node ./node_modules/mocha/bin/mocha ./dist/test/integration/*Test.js",
    "test-ts": "node ./node_modules/mocha/bin/mocha ts-node/register ./src/test/unit/*Test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
