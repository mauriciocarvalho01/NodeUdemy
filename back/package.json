    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.13.2",
    "@types/node-schedule": "^1.2.3",
    "@types/ramda": "^0.26.8",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/shortid": "0.0.29",
